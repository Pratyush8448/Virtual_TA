{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/?",
      "methods": ["POST"],
      "dest": "main.py"
    },
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "main.py"
    }
  ]
}
