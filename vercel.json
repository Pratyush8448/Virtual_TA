{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "main.py"
    },
    {
      "src": "/.*",
      "dest": "main.py"
    }
  ]
}
